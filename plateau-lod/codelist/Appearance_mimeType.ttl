@prefix ns: <https://lod.geospatial.jp/codelists/Appearance_mimeType#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

ns:Appearance_mimeType a skos:ConceptScheme .

ns:image_jpg a skos:Concept ;
skos:inScheme ns:Appearance_mimeType ;
skos:prefLabel "*.jpeg, *.jpg images" .
ns:image_png a skos:Concept ;
skos:inScheme ns:Appearance_mimeType ;
skos:prefLabel "*.png images" .